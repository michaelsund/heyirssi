{
  "name": "heyirssi",
  "version": "1.0.0",
  "description": "Electron Angular2 irssi notification",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "dev": "concurrent \"npm run tsc:w\" \"npm start\" ",
    "packagewin": "electron-packager ./ HeyIrssi --out ./builds --version 0.36.7 --platform=win32 --arch=x64 --overwrite --icon=pics/heyirssi-icon-40x40.ico",
    "packagelin": "electron-packager ./ HeyIrssi --out ./builds --version 0.36.7 --platform=lin --arch=x64 --overwrite",
    "packageosx": "electron-packager ./ HeyIrssi --out ./builds --version 0.36.7 --platform=darwin --arch=x64 --overwrite --icon=./pics/heyirssi-icon-osx-40x40.icns"
  },
  "dependencies": {
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "node-notifier": "^4.4.0",
    "angular2": "2.0.0-beta.0",
    "systemjs": "0.19.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.7",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3"
  }
}
